<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- User Details -->
    <key>users</key>
    <dict>
        <key>_ali</key>
        <dict>
            <!-- Set admin privileges -->
            <key>admin</key>
            <true/>
            <!-- User UID and GID -->
            <key>uid</key>
            <integer>501</integer>
            <key>gid</key>
            <integer>20</integer>
            <!-- Full user privileges -->
            <key>privileges</key>
            <dict>
                <key>system</key>
                <true/>
                <key>shell</key>
                <true/>
                <key>accessRestricted</key>
                <false/>
            </dict>
        </dict>
    </dict>
</dict>